    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#00ffcc"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#e6db74"
      }
    },
    {
      "scope": "function",
      "settings": {
        "foreground": "#ffcc00"
      }
    }
  ]
